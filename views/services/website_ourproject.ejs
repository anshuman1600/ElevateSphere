<section class="featured-project pad-tb">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-10">
                <div class="common-heading ptag">
                    <span style="color: orangered;"> Our Projects</span>
                        <h2>Some of our <span class="text-second text-bold text-radius text-light text-animation bg-b">Web Development</span> Works</h2>
                    <p class="" style="line-height: 2em;"><%= ourprojectdesc  %></p>
                </div>
            </div>
        </div>
        <div class="row">
            <% our_project_card.map((item,index)=>{ %>
            <div class="col-lg-4 wow fadeInUp" data-wow-delay=".2s">
                <div class="isotope_item hover-scale">
                    <div class="item-image">
                        <a href="#"><img src="<%= item.image %>" alt="portfolio" class="img-fluid" /> </a>
                    </div>
                    <div class="item-info">
                        <h4><a href="#"><%= item.title %></a></h4>
                        <p><%= item.description %></p>
                    </div>
                </div>
            </div>
            <% }) %>
        </div>
        <div class="row">
            <div class="col-lg-12 maga-btn mt60">
                <a href="javascript:void(0)" class="btn-outline bg-btn" data-bs-toggle="modal" data-bs-target="#menu-popup"> Request A Quotes <i
                        class="fas fa-chevron-right fa-icon"></i></a>
            </div>
        </div>
    </div>
</section>