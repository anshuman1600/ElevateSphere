        .coming-soon .animation {
            position: relative;
            width: 10px;
            height: 10px;
            margin-left: 5px;
            animation: pulse 1s infinite;
            font-size: 12px;
        }

        @keyframes pulse {
            0% {
                transform: scale(1);
                opacity: 0.7;
                color: #ff4510;
            }

            50% {
                transform: scale(1.5);
                opacity: 1;
                color: #ff1010;
            }

            100% {
                transform: scale(1);
                opacity: 0.7;
                color: #ff4510;
            }
        }


